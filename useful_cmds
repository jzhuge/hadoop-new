export HADOOP_KEYSTORE_PASSWORD=password
kms.sh run

keytool -genkey -alias tomcat -keyalg RSA

Show curl ciphers:
curl -sS https://www.howsmyssl.com/a/check
curl --ciphers 'ALL!EXPORT!eNULL!aNULL!LOW!RC4' https://www.howsmyssl.com/a/check

http://HOST:PORT/kms/v1/keys/names

openssl s_client -connect localhost:9600 -showcerts </dev/null 2>/dev/null | sed -n '/^-----BEGIN CERT/,/^-----END CERT/p' > localhost.pem 
curl --cacert localhost.pem --negotiate -u: -sS 'https://localhost:9600/webhdfs/v1/?op=liststatus'
curl --cacert localhost.pem --negotiate -u: -sS 'https://localhost:9600/webhdfs/v1/?op=liststatus' | python -m json.tool

hadoop key list -provider kms://http@localhost:9600/kms
hadoop key create new1 -provider kms://http@localhost:9600/kms

hadoop key list -provider kms://https@localhost:9600/kms
hadoop key create new1 -provider kms://https@localhost:9600/kms

